platform :ios, '8.0'

# ignore all warnings from all pods
inhibit_all_warnings!

# use frameworks for Swift
use_frameworks!

# Twitter Login
pod 'TwitterKit'

# Restaurants (FourSquare)
pod 'QuadratTouch'

# Autolayout
pod 'SnapKit'

# UI & Animations
pod 'RPCircularProgress'

# Models
pod 'CottonObject'

# Image Downloading
pod 'SDWebImage'

# Random data (for prototyping)
pod 'SwiftRandom'

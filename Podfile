platform :ios, '8.0'

# ignore all warnings from all pods
inhibit_all_warnings!

# use frameworks for Swift
use_frameworks!

# Twitter Login
pod 'TwitterKit'

# Networking
pod 'Alamofire'

# Autolayout
pod 'SnapKit'

# Models
pod 'CottonObject', :git => 'git@github.com:iwasrobbed/CottonObject.git', :branch => 'master'

# Image Downloading
pod 'SDWebImage'